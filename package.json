{
	"name": "file-manager-server",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"start": "NODE_ENV=production node --es-module-specifier-resolution=node dist/index.js",
		"dev": "NODE_ENV=development nodemon --ext ts --exec node --loader ts-node/esm --es-module-specifier-resolution=node src/index.ts",
		"start:dev": "NODE_ENV=development node --loader ts-node/esm --es-module-specifier-resolution=node src/index.ts",
		"start:prod": "NODE_ENV=production node --es-module-specifier-resolution=node dist/index.js",
		"start:test": "NODE_ENV=test node --loader ts-node/esm --es-module-specifier-resolution=node src/index.ts",
		"build": "rimraf dist && tsc",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"test": "NODE_ENV=test vitest run",
		"test:server": "NODE_ENV=test vitest run src/__tests__/server.test.ts",
		"test:watch": "NODE_ENV=test vitest",
		"test:coverage": "NODE_ENV=test vitest run --coverage",
		"test:file": "NODE_ENV=test vitest run",
		"test:ui": "NODE_ENV=test vitest --ui"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"express-rate-limit": "^7.5.0",
		"helmet": "^8.1.0",
		"hpp": "^0.2.3",
		"mime-types": "^2.1.35",
		"multer": "^1.4.5-lts.2",
		"uuid": "^11.1.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/dotenv": "^8.2.3",
		"@types/express": "^5.0.1",
		"@types/express-fileupload": "^1.5.1",
		"@types/helmet": "^4.0.0",
		"@types/hpp": "^0.2.6",
		"@types/mime-types": "^2.1.4",
		"@types/mock-fs": "^4.13.4",
		"@types/multer": "^1.4.12",
		"@types/node": "^20.12.0",
		"@types/sinon": "^17.0.4",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^10.0.0",
		"@vitest/ui": "^3.0.9",
		"c8": "^8.0.1",
		"chai": "^5.2.0",
		"express-fileupload": "^1.5.1",
		"mock-fs": "^5.5.0",
		"nodemon": "^3.1.9",
		"rimraf": "^6.0.1",
		"sinon": "^19.0.4",
		"supertest": "^6.3.4",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.3.3",
		"vitest": "^3.0.9"
	}
}
